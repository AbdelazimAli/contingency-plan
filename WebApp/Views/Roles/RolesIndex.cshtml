
@using System.Web.Script.Serialization
@using WebApp.Extensions
<br>
@{
    var admin = User.Identity.CanCustomize();
    var rtl = User.Identity.RTL();
    var grid = ViewData["GridName"];
    var count = ViewBag.count;
    Layout = null;
    int dataLevel = 0, read = 0;
    int.TryParse(Request.QueryString["DataLevel"], out dataLevel);
    int.TryParse(Request.QueryString["Read"], out read);
}

<div id="pageTabs"></div>
<script>
    FormJs.DrawTabs("pageTabs");

    $('#tab_Roles').load("@Url.Action("Index", "Roles")?MenuId=@Request.QueryString["MenuId"]&RoleId=@Request.QueryString["RoleId"]&Version=@Request.QueryString["Version"]&DataLevel=@Request.QueryString["DataLevel"]&Read=@Request.QueryString["Read"]");

    //صلاحيات المنظومة
    $("#t_Roles").click(function () {
        $("#tab_SSRole").empty();
        $('#tab_Roles').load("@Url.Action("Index", "Roles")?MenuId=@Request.QueryString["MenuId"]&RoleId=@Request.QueryString["RoleId"]&Version=@Request.QueryString["Version"]&DataLevel=@Request.QueryString["DataLevel"]&Read=@Request.QueryString["Read"]");
    });
    // صلاحيات الخدمة الذاتيه
    $("#t_SSRole").click(function () {
        $("#tab_Roles").empty();
        $('#tab_SSRole').load("@Url.Action("Index", "Roles")?MenuId=@Request.QueryString["MenuId"]&RoleId=@Request.QueryString["RoleId"]&Version=@Request.QueryString["Version"]&DataLevel=@Request.QueryString["DataLevel"]&Read=@Request.QueryString["Read"]");
    });
</script>









